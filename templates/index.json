{
  "sections": {
    "rich-text": {
      "type": "rich-text",
      "blocks": {
        "heading": {
          "type": "heading",
          "settings": {
            "heading": "<p>Introduce your brand or product with a bold statement.<\/p>",
            "heading_size": "hxl"
          }
        }
      },
      "block_order": [
        "heading"
      ],
      "settings": {
        "desktop_content_position": "center",
        "content_alignment": "center",
        "color_scheme": "background-1",
        "full_width": false,
        "padding_top": 40,
        "padding_bottom": 60
      }
    },
    "featured-product": {
      "type": "featured-product",
      "blocks": {
        "title": {
          "type": "title",
          "settings": {
            "heading_size": "h2"
          }
        },
        "price": {
          "type": "price",
          "settings": {
          }
        },
        "text": {
          "type": "text",
          "settings": {
            "text": "Enter a description of your featured product. Make sure to mention what makes it special, and what sets it apart from the competition.",
            "text_style": "subtitle"
          }
        },
        "variant_picker": {
          "type": "variant_picker",
          "settings": {
            "picker_type": "button"
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": true
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "share_label": "Share"
          }
        }
      },
      "block_order": [
        "title",
        "price",
        "text",
        "variant_picker",
        "buy_buttons",
        "share"
      ],
      "settings": {
        "product": "",
        "color_scheme": "background-1",
        "secondary_background": false,
        "media_size": "medium",
        "constrain_to_viewport": true,
        "media_fit": "contain",
        "media_position": "left",
        "image_zoom": "lightbox",
        "hide_variants": false,
        "enable_video_looping": true,
        "padding_top": 32,
        "padding_bottom": 48
      }
    },
    "image-banner": {
      "type": "image-banner",
      "settings": {
        "image_overlay_opacity": 0,
        "image_height": "small",
        "desktop_content_position": "middle-center",
        "show_text_box": true,
        "desktop_content_alignment": "center",
        "color_scheme": "background-1",
        "mobile_content_alignment": "center",
        "stack_images_on_mobile": true,
        "show_text_below": true
      }
    }
  },
  "order": [
    "rich-text",
    "featured-product",
    "image-banner"
  ]
}